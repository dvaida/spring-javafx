<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import tornadofx.control.DateTimePicker?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox layoutX="10.0" layoutY="10.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox>
               <children>
                  <HBox>
                     <children>
                        <Label text="Records">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </Label>
                        <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
                           <children>
                              <Button fx:id="unselectBtn" mnemonicParsing="false" text="Unselect" />
                           </children>
                        </HBox>
                     </children>
                  </HBox>
                  <ListView fx:id="listview" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox spacing="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <HBox>
                     <children>
                        <Label text="Properties">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </Label>
                        <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
                           <children>
                              <Button mnemonicParsing="false" text="Clear" />
                           </children>
                        </HBox>
                     </children>
                  </HBox>
                  <HBox spacing="5.0">
                     <children>
                        <Label maxWidth="-Infinity" minWidth="-Infinity" prefWidth="120.0" text="Numeric" />
                        <TextField fx:id="numericTextField" />
                     </children>
                  </HBox>
                  <HBox spacing="5.0">
                     <children>
                        <Label maxWidth="-Infinity" minWidth="-Infinity" prefWidth="120.0" text="TextField" />
                        <TextField fx:id="stringTextField" />
                     </children>
                  </HBox>
                  <HBox spacing="5.0">
                     <children>
                        <Label maxWidth="-Infinity" minWidth="-Infinity" prefWidth="120.0" text="File TextField" />
                        <TextField fx:id="fileTextField" />
                        <Button fx:id="selectFileBtn" mnemonicParsing="false" text="Select" />
                     </children>
                  </HBox>
                  <HBox spacing="5.0">
                     <children>
                        <Label maxWidth="-Infinity" minWidth="-Infinity" prefWidth="120.0" text="Date Picker" />
                        <DateTimePicker fx:id="datepicker" prefHeight="25.0" prefWidth="148.0" />
                     </children>
                  </HBox>
                  <CheckBox fx:id="selectCheckBox" mnemonicParsing="false" text="Select" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
