<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:lang="http://www.springframework.org/schema/lang"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:rm="http://www.mycompany.com/schema/rm"
       
        
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
          http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-5.0.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-5.0.xsd
          http://www.springframework.org/schema/lang http://www.springframework.org/schema/lang/spring-lang-5.0.xsd
          http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-5.0.xsd
          http://www.mycompany.com/schema/rm http://www.mycompany.com/schema/rm/rm.xsd">
      
  
  
  
  <!-- Connections --> 
  <rm:treemodel id="sampletreemodel">
    <rm:datasources> 
      <rm:datasource-ref level="0" ref="stationsDataSource"/> 
      <rm:datasource-ref level="1" ref="stationSensorsDataSource"/> 
    </rm:datasources> 
    <rm:links>
      <rm:link childLevel="1" parentField="station_id" childField="station_id"/>
    </rm:links> 
  </rm:treemodel>
  
  <rm:treeview id="sampletree" 
               fxml="fxml/TreeSample.fxml"
               fxmlId="treeview"
               treemodelRef="sampletreemodel">
    <rm:level-cellfactory level="0" textField="station_name"/> 
    <rm:level-cellfactory level="1" textField="sensor_def"/> 
    
  </rm:treeview> 
</beans>
